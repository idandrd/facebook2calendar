{
  "name": "facebook2calendar",
  "version": "1.2",
  "description": "Add facebook events to your google calendar",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "js": ["main.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab"]
}
